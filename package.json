{
  "name": "payw.org",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build && npm run add-domain",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
